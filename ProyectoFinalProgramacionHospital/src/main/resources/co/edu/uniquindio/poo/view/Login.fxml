<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="164.0" prefWidth="367.0" xmlns="http://javafx.com/javafx/17.0.6" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.controller.LoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="164.0" prefWidth="420.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="txtNombre" layoutX="116.0" layoutY="30.0" />
            <Text layoutX="115.0" layoutY="26.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nombre:" />
            <Text layoutX="115.0" layoutY="84.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ID:" />
            <TextField fx:id="txtId" layoutX="116.0" layoutY="88.0" />
            <Button fx:id="btnIngresar" layoutX="122.0" layoutY="124.0" mnemonicParsing="false" onAction="#verificarAcceso" text="ingresar" />
            <Button fx:id="btnRegistrarse" layoutX="220.0" layoutY="124.0" mnemonicParsing="false" onAction="#abrirRegistro" text="Registrarse" />
      </children>
    </AnchorPane>
  </children>
</VBox>
